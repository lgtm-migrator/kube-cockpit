{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@kube-cockpit/*": [
        "libs/*"
      ],
      "@default/*": [
        "apps/webapp/src/app/*"
      ],
      "@env/*": [
        "apps/webapp/src/environments/*"
      ],
      "@kube-cockpit/navigator": [
        "libs/navigator/src/index.ts"
      ],
      "@kube-cockpit/socketio-plugin": [
        "libs/socketio-plugin/src/index.ts"
      ],
      "@kube-cockpit/tree": [
        "libs/tree/src/index.ts"
      ],
      "@kube-cockpit/home": [
        "libs/home/src/index.ts"
      ],
      "@kube-cockpit/dashboard": [
        "libs/dashboard/src/index.ts"
      ],
      "@kube-cockpit/not-found": [
        "libs/not-found/src/index.ts"
      ],
      "@kube-cockpit/experiments": [
        "libs/experiments/src/index.ts"
      ],
      "@kube-cockpit/widgets": [
        "libs/widgets/src/index.ts"
      ],
      "@kube-cockpit/material": [
        "libs/material/src/index.ts"
      ],
      "@kube-cockpit/animations": [
        "libs/animations/src/index.ts"
      ],
      "@kube-cockpit/utils": [
        "libs/utils/src/index.ts"
      ],
      "@kube-cockpit/auth": [
        "libs/auth/src/index.ts"
      ],
      "@kube-cockpit/shared": [
        "libs/shared/src/index.ts"
      ],
      "@kube-cockpit/app-confirm": [
        "libs/app-confirm/src/index.ts"
      ],
      "@kube-cockpit/draggable": [
        "libs/draggable/src/index.ts"
      ],
      "@kube-cockpit/breadcrumbs": [
        "libs/breadcrumbs/src/index.ts"
      ],
      "@kube-cockpit/scroll-to-top": [
        "libs/scroll-to-top/src/index.ts"
      ],
      "@kube-cockpit/scrollbar": [
        "libs/scrollbar/src/index.ts"
      ],
      "@kube-cockpit/context-menu": [
        "libs/context-menu/src/index.ts"
      ],
      "@kube-cockpit/theme-picker": [
        "libs/theme-picker/src/index.ts"
      ],
      "@kube-cockpit/quickpanel": [
        "libs/quickpanel/src/index.ts"
      ],
      "@kube-cockpit/ngx-pipes": [
        "libs/ngx-pipes/src/index.ts"
      ],
      "@kube-cockpit/svg-viewer": [
        "libs/svg-viewer/src/index.ts"
      ],
      "@kube-cockpit/led": [
        "libs/led/src/index.ts"
      ],
      "@kube-cockpit/chat-bot": [
        "libs/chat-bot/src/index.ts"
      ],
      "@kube-cockpit/json-diff": [
        "libs/json-diff/src/index.ts"
      ],
      "@kube-cockpit/toolbar": [
        "libs/toolbar/src/index.ts"
      ],
      "@kube-cockpit/sidenav": [
        "libs/sidenav/src/index.ts"
      ],
      "@kube-cockpit/loading-overlay": [
        "libs/loading-overlay/src/index.ts"
      ],
      "@kube-cockpit/grid": [
        "libs/grid/src/index.ts"
      ],
      "defer-load/*": [
        "dist/defer-load/*"
      ],
      "@kube-cockpit/defer-load": [
        "libs/defer-load/src/index.ts"
      ],
      "namespace/*": [
        "dist/namespace/*"
      ],
      "@kube-cockpit/namespace": [
        "libs/namespace/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
